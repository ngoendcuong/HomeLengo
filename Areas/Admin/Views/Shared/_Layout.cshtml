                                <div class="inner-header">
                                    <div class="inner-header-left">
                                        <div class="logo-box flex">
                                            <div class="logo"><a asp-controller="Home" asp-action="Index"><img src="~/assets/images/logo/logo@2x.png" alt="logo" width="166" height="48"></a></div>
                                            <div class="button-show-hide">
                                                <span class="icon icon-categories"></span>
                                            </div>
                                        </div>
                                        <div class="nav-outer flex align-center">
                                            <!-- Main Menu -->
                                            @await Component.InvokeAsync("MenuTop")
                                            <!-- Main Menu End-->
                                        </div>
                                    </div>


                                    <div class="inner-header-right header-account">
                                        @if (Context.Session.GetString("UserId") != null)
                                        {
                                            var avatarSession = Context.Session.GetString("Avatar");
                                            var avatarFileName = !string.IsNullOrEmpty(avatarSession) ? avatarSession : "avatarMacDinh.jpg";
                                            var avatarPath = $"~/assets/images/avatar/{avatarFileName}";
                                            <a href="/Admin/Home" class="tf-btn btn-line btn-login" style="display: flex; align-items: center; gap: 8px;">
                                                <img src="@Url.Content(avatarPath)" alt="avatar" style="width: 24px; height: 24px; border-radius: 50%; object-fit: cover;" onerror="this.onerror=null; this.src='@Url.Content("~/assets/images/avatar/avatarMacDinh.jpg")'">
                                                <span>@Context.Session.GetString("FullName")</span>
                                            </a>
                                        }
