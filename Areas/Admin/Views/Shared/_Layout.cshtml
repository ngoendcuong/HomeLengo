    <style>
        .sidebar-menu-dashboard .box-menu-dashboard .nav-menu-item form {
            width: 100%;
            margin: 0;
            padding: 0;
        }
        .sidebar-menu-dashboard .box-menu-dashboard .nav-menu-item:hover form button.nav-menu-link,
        .sidebar-menu-dashboard .box-menu-dashboard .nav-menu-item form button.nav-menu-link:hover {
            background-color: #1563df !important;
        }
        .sidebar-menu-dashboard .box-menu-dashboard .nav-menu-item:hover form button.nav-menu-link svg g,
        .sidebar-menu-dashboard .box-menu-dashboard .nav-menu-item form button.nav-menu-link:hover svg g {
            opacity: 1 !important;
        }
        .sidebar-menu-dashboard .user-box .user .icon-box {
            overflow: hidden;
        }
        .sidebar-menu-dashboard .user-box .user .icon-box img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            display: block;
        }
        
        /* Status Colors */
        .btn-status.status-for-sale {
            background-color: #28a745 !important;
            color: white !important;
        }
        .btn-status.status-sold {
            background-color: #dc3545 !important;
            color: white !important;
        }
        .btn-status.status-for-rent {
            background-color: #007bff !important;
            color: white !important;
        }
        .btn-status.status-pending {
